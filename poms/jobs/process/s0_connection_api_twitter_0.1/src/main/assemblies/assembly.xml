                <include>org.talend.libraries:opensaml-saml-impl-3.3.0:jar:6.0.0</include>
                <include>org.talend.libraries:wss4j-ws-security-policy-stax-2.2.4:jar:6.0.0</include>
                <include>org.talend.libraries:ehcache-2.10.6:jar:6.0.0</include>
                <include>org.talend.libraries:wss4j-ws-security-stax-2.2.4:jar:6.0.0</include>
                <include>org.talend.libraries:spring-aop-5.1.4.RELEASE:jar:6.0.0</include>
                <include>org.talend.libraries:spring-context-5.1.4.RELEASE:jar:6.0.0</include>
                <include>org.talend.libraries:spring-core-5.1.4.RELEASE:jar:6.0.0</include>
