                <include>org.example.analysis_twitter.code:routines:jar:7.3.1</include>
                <include>org.talend.libraries:json_simple-1.1:jar:6.0.0</include>
                <include>org.talend.libraries:opensaml-xacml-impl-3.3.0:jar:6.0.0</include>
            </includes>
            <outputFileNameMapping>${artifact.artifactId}.${artifact.extension}</outputFileNameMapping>
            <useProjectArtifact>false</useProjectArtifact>
        </dependencySet>
        <dependencySet>
            <outputDirectory>lib</outputDirectory>
            <includes>
                <include>org.apache.logging.log4j:log4j-slf4j-impl:jar:2.12.1</include>
                <include>org.bouncycastle:bcprov-jdk15on:jar:1.60</include>
